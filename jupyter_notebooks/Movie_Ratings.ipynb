    "L = [('abc',10),('def',15),('ghi',10),('abc', 12),('xyz',100)]\n",
    "def top10(L):\n",
    "    L.sort(key = lambda x:x[1], reverse=True)\n",
    "    print(\"Sorted by Value Descending\", [i for i, _ in L[:10]])"
   ]
